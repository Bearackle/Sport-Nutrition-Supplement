        "/api/products/create": {
            "post": {
                "tags": [
                    "Product"
                ],
                "summary": "Tạo một sản phẩm",
                "description": "Tạo sản phẩm mới gửi cùng với ảnh, lưu ý hành động này cũng sinh ra một variant mặc định tasteless",
